{
  "name": "kf-vue-toast-plugin",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/kf-vue-toast-plugin.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BKeanu1989/vue-custom-toasts-plugin"
  },
  "author": {
    "name": "Kevin Fechner",
    "email": "developer.kevinfechner@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:declaration": "vue-tsc --declaration --emitDeclarationOnly",
    "release": "npm run build && npx standard-version && git push --follow-tags origin master && npm publish"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@vueuse/integrations": "^9.13.0",
    "nprogress": "^0.2.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  }
}
